{
  "server": {
    "host": "0.0.0.0",
    "port": 8080,
    "threads": 4,
    "tls": {
      "enabled": false,
      "certificate": "config/ssl/server.crt",
      "private_key": "config/ssl/server.key"
    },
    "limits": {
      "max_body_bytes": 268435456
    }
  },
  "storage": {
    "base_path": "data",
    "temp_path": "data/tmp"
  },
  "observability": {
    "log_level": "information"
  }
}
